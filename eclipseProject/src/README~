Para compilar todos los tests:
	javac Dominio/CtrlDominioTest.java Dominio/CtrlDataTest.java Dominio/HeteSimTest.java Dominio/HeteSanicTest.java resultTesting/*.java
	

CtrlDominioTest:
	java Dominio.CtrlDominioTest ../../GrafsAndResults/

CtrlDataTest:
	java Dominio.CtrlDataTest

HeteSimTest:
	java Dominio.HeteSimTest ../../GrafsAndResults/

HeteSanicTest:
	java Dominio.HeteSanicTest ../../GrafsAndResults/

ResultTest:
	java resultTesting.ResultTest
